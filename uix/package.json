{
  "name": "@bootstrapp/uix",
  "version": "0.2.0",
  "description": "UI/UX component toolkit built on @bootstrapp/view with ready-to-use components for modern web applications",
  "main": "./index.js",
  "module": "./index.js",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./import.js": "./import.js",
    "./display/*": "./display/*.js",
    "./layout/*": "./layout/*.js",
    "./form/*": "./form/*.js",
    "./navigation/*": "./navigation/*.js",
    "./overlay/*": "./overlay/*.js",
    "./feedback/*": "./feedback/*.js",
